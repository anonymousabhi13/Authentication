<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title>Hello, world!</title>
</head>

<body>
    <nav class="navbar  navbar-light bg-light">
        <button type="submit" class="btn btn-danger mx-5"><a style="color: black; text-decoration: none;"
                href="/logout">logout</a></button>
                <button type="submit" class="btn btn-danger mx-5"><a style="color: black; text-decoration: none;"
                    href="/profile">back</a></button>
        </div>
    </nav>
    <% gg.forEach(function(show){%>
        <div class="container my-3 d-flex">

            <div class="card" style="width: 18rem;">
                <img src="<%=show.image%>" class="card-img-top" alt="...">
                <div class="card-body">
                    <a href="/like/<%= show._id %>" class="btn btn-primary"> <%= show.like.length %> like</a>
                    <p class="card-text"><%=show.product%></p>
                </div>
            </div>
        </div>
        <%})%>
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
                integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
                crossorigin="anonymous"></script>
</body>

</html>